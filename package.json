{
  "name": "ui-testing",
  "version": "1.0.0",
  "description": "SAPUI5 User Interface Testing - Example SAPUI5 application demonstrating the use of QUnit, OPA5, karma and BackstopJS.",
  "main": "server.js",
  "scripts": {
    "test": "npm run -s backstop && npm run -s karma",
    "backstop": "node backstop.js",
    "backstop-approve": "backstop approve",
    "karma": "karma start --single-run --browsers ChromeHeadless",
    "tdd": "karma start"
  },
  "keywords": [
    "SAPUI5",
    "OpenUI5",
    "QUnit",
    "OPA5",
    "karma",
    "BackstopJS",
    "Testing",
    "JavaScript",
    "Node.js"
  ],
  "author": {
    "name": "Christian Schuff",
    "email": "christian.schuff@ui5experts.com",
    "url": "http://www.ui5experts.com"
  },
  "dependencies": {},
  "devDependencies": {
