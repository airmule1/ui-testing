    "express": "^4.15.4",
    "http-proxy-middleware": "^0.17.4",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-junit-reporter": "^1.2.0",
    "karma-openui5": "^0.2.2",
    "karma-qunit": "^1.2.1",
